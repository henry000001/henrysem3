<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="henrysem3.MainPage">

    <StackLayout>
        <Frame BackgroundColor="#2196F3" Padding="24" CornerRadius="0">
            <Label Text="Welcome to Xamarin.Forms!" HorizontalTextAlignment="Center" TextColor="White" FontSize="36"/>
        </Frame>
        <Image Source="http://www.bancodeideas.gob.ec/files/incubadora/logo-uisrael20170801093912.png"/>
        <Image Source=""/>
        <Entry x:Name="txtusuario" Placeholder="Ingrese el usuario"/>
        <Entry x:Name="txtcontraseña" Placeholder="Ingrese la contraseña" IsPassword="True"/>
        <Button x:Name="btnabrir" Text="Abrir Pagina2 " Clicked="btnabrir_Clicked" />
    </StackLayout>

</ContentPage>
